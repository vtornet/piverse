#, python-format
msgid "Error al guardar la imagen de la publicación: %(error)s"
msgstr "Error saving the post image: %(error)s"

#: app.py:1191
msgid "Tipo de archivo de imagen no permitido. Permitidos: png, jpg, jpeg, gif."
msgstr "Image file type not allowed. Allowed: png, jpg, jpeg, gif."

#: app.py:1222
msgid "Publicación creada."
msgstr "Post created."

